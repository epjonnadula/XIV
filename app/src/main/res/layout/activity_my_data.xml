<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="innovision.jonnadulaprithvi.xiv.MyDataActivity"
    android:background="@drawable/profile">


    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/mydata_name"
        android:layout_alignParentTop="true"
        android:layout_marginTop="165dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="@android:color/transparent"
        android:enabled="false"
        android:textColor="#2a2f3e"
        android:maxLength="23"/>
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/mydata_id"
        android:layout_alignParentTop="true"
        android:layout_marginTop="205dp"
        android:layout_alignRight="@+id/mydata_name"
        android:layout_alignEnd="@+id/mydata_name"
        android:background="@android:color/transparent"
        android:enabled="false"
        android:textColor="#2a2f3e"/>
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/mydata_college"
        android:layout_alignParentTop="true"
        android:layout_marginTop="255dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="@android:color/transparent"
        android:enabled="false"
        android:textColor="#2a2f3e"
        android:maxLength="23"/>
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/mydata_contact"
        android:layout_alignParentTop="true"
        android:layout_marginTop="300dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="@android:color/transparent"
        android:enabled="false"
        android:textColor="#2a2f3e"
        android:maxLength="15"/>
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/mydata_bucks"
        android:layout_alignParentTop="true"
        android:layout_marginTop="350dp"
        android:layout_alignRight="@+id/mydata_contact"
        android:layout_alignEnd="@+id/mydata_contact"
        android:background="@android:color/transparent"
        android:enabled="false"
        android:textColor="#2a2f3e"/>


</RelativeLayout>
